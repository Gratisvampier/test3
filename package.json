{
	"name": "ttest",
	"version": "1.0.0",
	"scripts": {
		"build": "npm run clean && webpack --config ./webpack.config.js --mode production --progress",
		"dev": "npm run clean && webpack --config ./webpack.config.js --mode development",
		"dev:watch": "npm run clean && webpack --config ./webpack.config.js --mode development --watch",
		"start": "webpack serve --config ./webpack.config.js --mode development --host 0.0.0.0",
		"clean": "rimraf ./dist"
	},
	"dependencies": {
		"autoprefixer": "10.3.7",
		"goober": "2.0.41",
		"postcss": "8.3.9",
		"preact": "10.5.13"
	},
	"devDependencies": {
		"@babel/core": "7.15.8",
		"@babel/plugin-transform-react-jsx": "7.14.9",
		"@babel/preset-env": "7.15.8",
		"@babel/preset-react": "7.14.5",
		"babel-loader": "8.2.2",
		"copy-webpack-plugin": "8.0.0",
		"css-loader": "6.4.0",
		"eslint": "7.21.0",
		"eslint-config-standard-preact": "1.1.6",
		"eslint-loader": "4.0.2",
		"eslint-plugin-optimize-regex": "1.2.0",
		"eslint-plugin-preact": "0.1.0",
		"eslint-plugin-promise": "4.3.1",
		"eslint-plugin-sonarjs": "0.6.0",
		"file-loader": "6.2.0",
		"html-loader": "2.1.2",
		"html-webpack-plugin": "5.3.2",
		"prettier": "2.4.1",
		"rimraf": "3.0.2",
		"style-loader": "3.3.0",
		"webpack": "5.58.1",
		"webpack-bundle-analyzer": "^4.4.2",
		"webpack-cli": "4.5.0",
		"webpack-dev-server": "3.11.2"
	}
